{
  "name": "herculesteam-augeasproviders_grub",
  "version": "4.0.0",
  "author": "Raphael Pinson, Trevor Vaughan",
  "summary": "Augeas-based grub types and providers for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/hercules-team/augeasproviders_grub",
  "project_page": "http://augeasproviders.com",
  "issues_url": "https://github.com/hercules-team/augeasproviders_grub/issues",
  "description": "This module provides types/providers for grub configuration files using the Augeas configuration API library.",
  "dependencies": [
    {
      "name": "herculesteam/augeasproviders_core",
      "version_requirement": ">=2.4.0 < 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ]
}
